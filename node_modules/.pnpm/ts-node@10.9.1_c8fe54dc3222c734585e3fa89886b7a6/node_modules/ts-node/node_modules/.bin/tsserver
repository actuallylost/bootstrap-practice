#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/typescript@5.0.2/node_modules/typescript/bin/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/typescript@5.0.2/node_modules/typescript/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/typescript@5.0.2/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules:/Users/lost/Projects/techpro-exercises/src/node_modules:/Users/lost/Projects/techpro-exercises/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/ts-node@10.9.1_c8fe54dc3222c734585e3fa89886b7a6/node_modules/ts-node/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/ts-node@10.9.1_c8fe54dc3222c734585e3fa89886b7a6/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/typescript@5.0.2/node_modules/typescript/bin/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/typescript@5.0.2/node_modules/typescript/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/typescript@5.0.2/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules:/Users/lost/Projects/techpro-exercises/src/node_modules:/Users/lost/Projects/techpro-exercises/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/ts-node@10.9.1_c8fe54dc3222c734585e3fa89886b7a6/node_modules/ts-node/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/ts-node@10.9.1_c8fe54dc3222c734585e3fa89886b7a6/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@5.0.2/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@5.0.2/node_modules/typescript/bin/tsserver" "$@"
fi
