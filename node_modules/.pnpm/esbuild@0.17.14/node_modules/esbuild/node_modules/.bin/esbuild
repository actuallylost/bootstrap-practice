#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/esbuild@0.17.14/node_modules/esbuild/bin/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/esbuild@0.17.14/node_modules/esbuild/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/esbuild@0.17.14/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules:/Users/lost/Projects/techpro-exercises/src/node_modules:/Users/lost/Projects/techpro-exercises/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/esbuild@0.17.14/node_modules/esbuild/bin/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/esbuild@0.17.14/node_modules/esbuild/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/esbuild@0.17.14/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules/.pnpm/node_modules:/Users/lost/Projects/techpro-exercises/src/bootstrap-practice/node_modules:/Users/lost/Projects/techpro-exercises/src/node_modules:/Users/lost/Projects/techpro-exercises/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
